{
    "name": "babdev/transifex",
    "description": "The Transifex API Package is a wrapper of the Transifex API available for PHP developers",
    "keywords": ["php", "transifex"],
    "homepage": "https://github.com/BabDev/Transifex-API",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Michael Babker",
            "homepage": "https://www.babdev.com"
        }
    ],
    "require": {
        "php": "~7.2",
        "ext-json": "*",
        "psr/http-client": "~1.0",
        "psr/http-factory": "~1.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3.3",
        "http-interop/http-factory-guzzle": "~1.0",
        "phpunit/phpunit": "~8.0",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\Transifex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabDev\\Transifex\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
