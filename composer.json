{
    "name": "babdev/library",
    "description": "BabDev's PHP libraries",
    "keywords": ["php", "library"],
    "homepage": "https://github.com/BabDev/BabDev-Library",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Michael Babker",
            "homepage": "http://www.babdev.com"
        }
    ],
    "require": {
        "php": ">=5.3.10",
        "joomla/uri": "~1.0"
    },
    "require-dev": {
        "joomla/test": "~1.0",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabDev\\Tests\\": "tests/"
        }
    },
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	}
}
